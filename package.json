{
  "name": "userauth-organization",
  "version": "1.0.0",
  "description": "A User Authentication and Authorization application written with Nodejs/TypeScript express framework",
  "main": "dist/index.js",
  "scripts": {
    "start": "nodemon index.ts",
    "build": "rimraf dist && tsc",
    "ts.check": "tsc --project tsconfig.json",
    "add-build": "git add dist",
    "test": "jest"
  },
  "pre-commit": [
    "ts.check",
    "build",
    "add-build"
  ],
  "keywords": [
    "authentication",
    "authorization",
    "express",
    "nodejs",
    "typescript"
  ],
  "author": "Abdulrafiu Kehinde Lawal (Kennie Larkson)",
  "license": "MIT",
  "dependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.9",
    "axios": "^1.7.2",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "nodemon": "^3.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/supertest": "^6.0.2",
    "axios-mock-adapter": "^1.22.0",
    "jest": "^29.7.0",
    "pre-commit": "^1.2.2",
    "rimraf": "^5.0.7",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.5"
  }
}
